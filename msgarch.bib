@Article{石晓婷2011,
  author        = {石晓婷},
  title         = {网络会计的发展及其相关问题研究},
  journal       = {企业研究},
  year          = {2011},
  number        = {02},
  pages         = {37--39},
  __markedentry = {[Administrator:6]},
  abstract      = {随着计算机技术、网络技术的发展,网络会计成了整个网络坏境下的必然发展结果,其普及只是一个时间的问题。但是,虽然网络会计发展迅速势不可挡,因为其与传统会计核算方式、管理手段、信息处理等方面的不同,在此过程中存在诸多问题。据此,本文将对网络会计的发展进行简要介绍及分析,并在此基础上就发展中的一些问题进行讨论并提出系列建议。},
  keywords      = {网络会计;;发展问题;;对策},
}

@Article{王全睿2011,
