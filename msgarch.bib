@Article{田新时2004,
  author   = {田新时 and 郭海燕},
  title    = {极值理论在风险度量中的应用——基于上证180指数},
  journal  = {运筹与管理},
  year     = {2004},
  volume   = {13},
  number   = {1},
  pages    = {106-111},
  abstract = {精确度量风险是金融风险管理的关键问题.本文引入广义帕雷托分布 代替传统的正态分布等,精确描述金融收益的厚尾特征.并将基于广义帕雷托分布的VaR模型和其它模型方法,如GARCH(1,1)、 GARCH(1,1)-t、历史模拟法、方差-协方差方法,进行比较分析.实证研究表明,基于广义帕雷托分布的VaR模型比传统的模型方法更适合厚尾分布 高分位点的预测,并且其预测结果比较稳定.这使得基于广义帕雷托分布的VaR模型成为VaR度量方法中最稳健的方法之一.},
  keywords = {金融风险管理;Value-at-Risk;极值理论;GARCH模型},
}

