@Article{滕升楠2017,
  author        = {滕升楠 and 冯娟 and 李宁},
  title         = {湖北省国内旅游客源市场竞争态及其成因分析},
  journal       = {华中师范大学学报(自然科学版)},
  year          = {2017},
  volume        = {51},
  number        = {01},
  pages         = {130--136},
  __markedentry = {[Administrator:6]},
  abstract      = {国内旅游客源市场是评价一个区域旅游竞争力的重要指标.运用旅游业市场竞争态模型可以很好的在对旅游市场进行定量分析的基础上,准确把握各旅游市场所处的地位和态势,实现旅游市场合理有序的开发.本文应用旅游市场竞争态模型,分析了湖北省2006年~2014年国内旅游客源市场发展状况,并以此为基础,着重从旅游吸引力及区域竞合关系等方面分析了竞争态总体格局产生的内在原因,提出了促进湖北省国内旅游市场发展的相关建议,对未来湖北省旅游业的健康稳定发展具有很好的参考和借鉴意义.},
  keywords      = {旅游市场竞争态;;国内旅游市场;;湖北省},
}

@Article{张瑞雪2017,
  author        = {张瑞雪},
  title         = {网络会计对传统会计的影响及网络会计发展趋势分析},
  journal       = {市场研究},
  year          = {2017},
  number        = {01},
  pages         = {60--62},
  __markedentry = {[Administrator:6]},
  abstract      = {会计是随着社会生产的发展和经济管理的要求而产生、发展并不断完善起来的。信息化社会和电子商务的出现将我们带入了一个知识经济、网络经济的时代。与之相适应的未来的会计模式必将以网络会计的形式出现。网络会计是网络环境下对各种交易和事件的确认、计量和披露的会计活动。它是基于网络环境下的会计信息系统,是电子商务的重要组成部分。在网络环境下,作为企业管理核心的财务管理也由传统的桌面财务走向网络财务。本文主要研究网络会计对传统会计的影响,分析网络会计的发展趋势。},
  keywords      = {网络会计;;传统会计;;发展趋势},
}

@Article{焦子宸2016,
  author        = {焦子宸},
  title         = {山西省旅游收入影响因素的计量分析},
  journal       = {天津商务职业学院学报},
  year          = {2016},
  volume        = {4},
  number        = {05},
  pages         = {32--34},
  __markedentry = {[Administrator:6]},
  abstract      = {伴随着社会现代化水平的提高,人们对旅游业的需求也在不断的增加,旅游业在第三产业中的地位越来越重要,旅游收入成为地区收入的一项重要来源。本文主要分析了山西省旅游业发展的概况,并运用计量经济学模型,对山西省旅游业总收入的影响因素进行研究,据研究结果显示,在影响山西省旅游业收入的因素中,人均国内生产总值和国内游客数量对山西省旅游业总收入影响较大,而公路里程和铁路里程数、星级酒店数对增加山西省旅游业收入的作用较小。因此,山西省旅游业发展,应重点提升景区景点知名度,从吸引游客、增加旅游花费入手。},
  keywords      = {旅游业;;计量经济模型;;居民可支配收入},
}

@Article{韩复龄2016,
  author        = {韩复龄 and 王碧澄},
  title         = {融资融券对中国A股流动性、波动性影响的实证研究——基于2015年股市异常波动的研究报告},
  journal       = {公司金融研究},
  year          = {2016},
  number        = {Z1},
  pages         = {68--101},
  __markedentry = {[Administrator:6]},
  abstract      = {从2015年6月开始,A股市场经历了前所未有的异常波动,其中学术界对场内场外杠杆交易的声讨最为突出。但是本文认为,场内融资融券由于具有独特的性质,不应被一视同仁地认为是造成股市巨幅波动的元凶。为了更有力地揭示场内融资融券对股市的影响,本研究选取股市代表性指数的流动性与波动性指标为考察对象,并且按照股市行情的变化分为四阶段进行实证分析。实证检验中通过场内融资融券与股市流动性、波动性的VaR模型、协整检验以及脉冲响应分析,发现在股市快速上涨与快速下跌阶段,融资融券交易对流动性与波动性的冲击方向更明朗、程度明显放大;并且融资融券交易总体上能够增加市场流动性,在股灾中能够平抑市场波动性。},
  keywords      = {股灾;;融资融券交易;;股市流动性、波动性;;脉冲响应模型},
}

@Article{刘振中2016,
  author        = {刘振中},
  title         = {基于多元线性回归模型的国内旅游消费分析},
  journal       = {重庆理工大学学报(自然科学)},
  year          = {2016},
  volume        = {30},
  number        = {06},
  pages         = {167--170},
  __markedentry = {[Administrator:6]},
  abstract      = {选取我国2003—2012年间的国内旅游消费情况相关数据,分析影响我国国内旅游消费水平的主要因素,建立并求解了以国内旅游消费为因变量,以居民人均可支配收入、游客人均消费和旅行社数量为自变量的多元线性回归模型。结果表明:国内旅游消费与居民可支配收入、游客人均消费呈现正相关关系,与旅行社数量呈现负相关关系。},
  keywords      = {国内旅游消费;;多元线性回归模型;;影响因素},
}

@Article{管河山2016,
  author        = {管河山 and 邹清明 and 罗智超},
  title         = {时间序列平稳性分类识别研究},
  journal       = {统计与信息论坛},
  year          = {2016},
  volume        = {31},
  number        = {04},
  pages         = {3--8},
  __markedentry = {[Administrator:6]},
  abstract      = {平稳性检验是时间序列回归分析的一个关键问题,已有的检验方法在处理海量时间序列数据时显得乏力,检验准确率有待提高。采用分类技术建立平稳性检验的新方法,可以有效地处理海量时间序列数据。首先计算时间序列自相关函数,构建一个充分非必要的判定准则;然后建立序列收敛的量化分析方法,研究收敛参数的最优取值,并提取平稳性特征向量;最后采用k-means聚类建立平稳性分类识别方法。采用一组模拟数据和股票数据进行分析,将ADF检验、PP检验、KPSS检验进行对比,实证结果表明新方法的准确率较高。},
  keywords      = {时间序列;;平稳性;;特征提取;;分类},
}

@Article{王安清2015,
  author        = {王安清},
  title         = {论网络会计对传统会计的影响及对策},
  journal       = {中国市场},
  year          = {2015},
  number        = {49},
  pages         = {146--147},
  __markedentry = {[Administrator:6]},
  abstract      = {随着互联网在我国各个领域的应用,很多企业都已经认识到网络会计是未来会计发展的必然趋势,网络会计处理业务效率高,且不受时间、空间上的限制,可以为全球任何客户提供服务,这对传统会计产生了巨大的影响。文章首先对网络会计的特点进行概述,其次分析网络会计对传统会计的影响,最后对网络会计的良性发展提出了一些对策。},
  keywords      = {网络会计;;传统会计;;会计信息},
}

@Article{韩跃2015,
  author        = {韩跃 and 马一鸣},
  title         = {浅谈网络时代对会计发展的影响},
  journal       = {科技经济市场},
  year          = {2015},
  number        = {05},
  pages         = {123--124},
  __markedentry = {[Administrator:6]},
  abstract      = {随着信息技术和网络技术的发展,世界已进入了网络信息时代,互联网已成为我们的办公室的理想平台,在网络经济的驱动下,会计的不断发展,产生了新的领域--网络会计。它打破了传统的会计管理模式,人们不再受地点、时间的约束,仅通过网络就可以传达信息数据,然而不可否认的是网络会计给我带来方便的同时也面临着许多问题。从内部上看,不利于审计人员对企业内部的审计、会计从业人员利用互联网舞弊等行为会导致会计信息的失真。从外部环境来看,网络会计法律不健全、会计制度滞后,计算机安全隐患等一系列的问题会导致会计信息的泄露,这些问题引发了我们的高度关注。因此,找到解决问题的方法,减少网络对会计信息带来的弊端。},
  keywords      = {网络时代;;信息;;会计发展;;采取措施},
}

@PhdThesis{蔡可佩2014,
  author        = {蔡可佩},
  title         = {人民币汇率波动对海外电力项目影响研究},
  school        = {华北电力大学},
  year          = {2014},
  __markedentry = {[Administrator:6]},
  abstract      = {随着经济全球化发展和我国金融市场的不断开放,人民币汇率变动对于整个国民经济和电力行业产生的影响越来越显著。自2005年汇改,人民币汇率经历了连续8年的连续升值。但2014年以来,由于受到美联储缩减QE(量化宽松政策)以及中国经济结构调整、增长速度趋缓等多重因素叠加的影响,人民币出现了持续贬值。目前决定人民币供求的经济基础状况基本平衡,未来人民币出现大幅度升值和贬值的经济基础都不存在,双向波动会成为常态。因此,在人民币汇率变动的新形势下,研究人民币汇率变动对海外电力项目的影响显得尤为重要。

本文以未来人民币汇率双向波动成为常态为假设,系统研究了人民币汇率变动对海外电力项目的影响,具体研究内容如下：

(1)结合今年以来人民币持续贬值的新情况,以未来人民币汇率双向波动成为常态为假设,对人民币汇率影响下的海外电力项目的发展进行了阐述,引出本文的主要研究方向。

(2)对世界电力行业的发展形势以及我国电力发展形势进行了简单介绍,提出电力项目主要是在世界电力行业和我国电力行业共同影响下发展的。通过分析发现,目前我国的电力行业得到了飞速的发展,我国超过美国成为世界上最大的电力消费国,这标志着我国综合国力的提高,同时对我国现阶段电力项目的投资也是一个挑战。从而引出我国发展电力项目的必要性。

(3)通过分析实际有效汇率、发电量、人均用电量、国内生产总值等因素对净出口电量的影响,建立海外电力项目中净出口电量的影响模型。为了确定模型中变量的参数,通过Eviews软件对模型的协整分析过程进行了介绍。

(4)对我国近十年的电力基本情况进行实证研究,通过对模型各参数的确定,表明人民币汇率是影响我国净出口电量的主要因素,并通过该案例验证了所建模型的可行性和准确性。},
  keywords      = {人民币汇率;;海外电力项目;;协整分析;;Eviews},
}

@Article{项卫星2014a,
  author        = {项卫星 and 李宏瑾},
  title         = {货币市场基准利率的性质及对Shibor的实证研究},
  journal       = {经济评论},
  year          = {2014},
  number        = {01},
  pages         = {107--117},
  __markedentry = {[Administrator:6]},
  abstract      = {作为我国金融市场基准利率体系建设和利率市场化改革配套的重要基础设施,上海银行间同业拆放利率(Shibor)已成功运行近7年。本文首先对货币市场基准利率的性质进行了讨论,指出市场代表性和基准性是金融市场基准利率的核心属性。对我国货币市场和Shibor运行以来情况的考察表明,Shibor具备了货币市场基准利率的基本特征。通过等均值、等方差检验、Granger因果分析和EGARCH模型等方法进行的实证分析表明,Shibor具有良好的市场代表性、基准性和稳定性,较好地发挥了货币市场基准利率的作用。今后应以市场机制建设为重点,进一步夯实巩固Shibor货币市场基准利率地位,通过Shibor的健全完善,推动中国金融市场化改革向纵深领域迈进,促进中国金融和经济的长远健康发展。},
  keywords      = {Shibor;;货币市场;;基准利率;;利率市场化},
}

@Article{戴国海2013,
  author        = {戴国海 and 李伟},
  title         = {SHIBOR在我国基准利率体系中的地位及其完善渠道研究},
  journal       = {金融监管研究},
  year          = {2013},
  number        = {06},
  pages         = {31--54},
  __markedentry = {[Administrator:6]},
  abstract      = {本文从基准利率体系的市场代表性、定价基础性、波动合理性、政策可控性和经济相关性五个基本属性出发,对我国基准利率体系的现状,特别是SHIBOR在其中的地位,进行了较为全面的评估。评估结果表明,现阶段SHIBOR、同业拆借利率和债券回购利率都无法单独承担货币市场基准利率的重任。货币市场实际的基准利率是由管制利率和市场利率混合组成的:SHIBOR、债券回购利率,构成了基准利率短端的核心;而同业拆借利率和存贷款管制利率则构成了基准利率的长端。在不同期限的基准利率中,1周的SHIBOR和2周的债券回购利率最有可能成为央行调控的目标基准利率。其中,1周的SHIBOR虽然政策可控性和经济相关性较强,但报价的非理性程度较高;而2周的债券回购利率虽然报价理性较强,但可控性和相关性都很差。},
  keywords      = {SHIBOR;;市场基准利率;;央行基准利率},
}

@Article{张杨玉婷2013,
  author        = {张杨玉婷},
  title         = {影响国内旅游业收入因素的分析},
  journal       = {沿海企业与科技},
  year          = {2013},
  number        = {03},
  pages         = {10--12},
  __markedentry = {[Administrator:6]},
  abstract      = {目前,旅游业正在发展为世界上最大和增长最快的产业之一。中国旅游产业已有长足的进步,并成为中国经济的支柱产业之一。为了更好地发挥旅游业的作用,有必要加强对旅游收入的计量分析。文章运用计量经济学知识,通过模型建立与调整,对影响我国国内旅游总收入的各种因素进行分析研究,揭示我国旅游业收入的特征,并对我国旅游业的发展提出对策和建议。},
  keywords      = {计量经济学;;国内旅游收入;;检验;;建议},
}

@Article{刘玉花2013,
  author        = {刘玉花},
  title         = {浅谈网络会计现状及发展策略},
  journal       = {市场论坛},
  year          = {2013},
  number        = {06},
  pages         = {69--70},
  __markedentry = {[Administrator:6]},
  abstract      = {近几年来随着电子商务的发展逐步壮大,给我们的政治、经济、文化带来了巨大变革。网络经济的发展为会计信息系统提供了全方位多角度的信息支持。互联网经济时代的到来,极大地改变了会计环境,使其面临着挑战和创新。为了更好地适应环境,网络会计随之产生,这样的会计环境的改变对于网络会计的发展起了关键性的作用。网络会计与传统会计不同,创新和发展了传统会计。但是网络会计在发展过程中还是会存在着各种不同的问题。为了使网络会计更好更健康的发展,怎样解决网络会计中存在的各种问题也越来越引起人们的关注。},
  keywords      = {网络会计;;会计信息;;问题;;策略},
}

@Article{吴清2013,
  author        = {吴清 and 张明 and 胡露露},
  title         = {湖北省国内旅游客源市场演化及其机制研究},
  journal       = {中南财经政法大学学报},
  year          = {2013},
  number        = {03},
  pages         = {151--156},
  __markedentry = {[Administrator:6]},
  abstract      = {客源市场研究是旅游规划和市场营销等工作的基础。以湖北省国内旅游抽样调查数据为基础,运用ADod指标、变异系数、位序～等级规模等方法分析2005～2012年湖北省国内客源市场的演化情况,研究结果表明:湖北客源地到主要目地的平均距离总体呈现出不断下降趋势,对国内远距离游客吸引逐渐减弱,客流分布区域趋于集中;其国内旅游客源市场相对差异不断缩小,表现稳定发展态势;其国内旅游市场规模分布差异程度逐渐缩小,中等规模市场增长较快,等级规模结构发育不够成熟。经济收入、闲暇时间、交通条件、旅游资源是影响湖北省国内客源市场演化的重要因素,其中,客源地经济水平差异和短期休闲增加是外在推手,交通条件改善和资源禀赋差异是内在变量。},
  keywords      = {湖北旅游业;;旅游客源市场;;旅游收入;;旅游市场营销},
}

@Article{毛润泽2012,
  author        = {毛润泽},
  title         = {中国区域旅游经济发展影响因素的实证分析},
  journal       = {经济问题探索},
  year          = {2012},
  number        = {08},
  pages         = {48--53},
  __markedentry = {[Administrator:6]},
  abstract      = {在界定中国区域旅游经济发展概念基础上,运用发展经济学理论,构建中国区域旅游经济发展影响因素分析模型,并利用2000～2009年省级面板数据,从全国、东部地区、中部地区和西部地区四个维度进行了实证检验。结论表明:区域经济发展水平和区域旅游经济非均衡程度是促进全国和三大地区旅游经济发展的重要正向因素;区域旅游基础设施在全国以及东部和中部地区促进旅游经济发展作用减缓,而在西部地区仍然是促进旅游经济发展的重要因素;区域产业结构水平仅在全国范围对旅游经济发展具有积极促进作用;区域制度环境还不健全,对旅游经济发展没有正向促进作用。因此,促进我国区域旅游经济发展需要贯彻执行分类指导的基本思想,实现宏观经济持续健康发展,增加居民可自由支配收入,提高旅游经济在区域经济中的地位,还需要协调旅游经济与其他第三产业经济的关系。},
  keywords      = {区域旅游经济发展;;影响因素;;计量模型;;面板数据},
}

@Article{郎需梅2012,
  author        = {郎需梅 and 代合治},
  title         = {山东省旅游经济影响因子的回归-关联分析},
  journal       = {云南地理环境研究},
  year          = {2012},
  volume        = {24},
  number        = {02},
  pages         = {53--57},
  __markedentry = {[Administrator:6]},
  abstract      = {目前,被称为"无烟工业"的旅游业正在迅速发展,山东省地处中国的东部沿海,交通便利,旅游资源丰富,这就为山东省旅游业的发展提供了良好的基础。根据山东省2006～2010年的国内旅游收入数据,运用线性回归分析和灰色关联分析相结合的方法,选取旅游经济支持、生态环境质量、旅游交通、旅游服务、社会文明程度和居民生活水平6个方面14个指标,对影响山东省国内旅游收入的相关因素及各因素的重要程度进行了分析。结果表明,国内旅游收入与居民生活水平联系最为密切,与交通因素关联性最弱。},
  keywords      = {线性回归分析;;灰色关联分析;;国内旅游收入;;山东省},
}

@Article{首招勇2012,
