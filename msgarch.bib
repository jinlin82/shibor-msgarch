@Article{石晓婷2011,
  author        = {石晓婷},
  title         = {网络会计的发展及其相关问题研究},
  journal       = {企业研究},
  year          = {2011},
  number        = {02},
  pages         = {37--39},
  __markedentry = {[Administrator:6]},
  abstract      = {随着计算机技术、网络技术的发展,网络会计成了整个网络坏境下的必然发展结果,其普及只是一个时间的问题。但是,虽然网络会计发展迅速势不可挡,因为其与传统会计核算方式、管理手段、信息处理等方面的不同,在此过程中存在诸多问题。据此,本文将对网络会计的发展进行简要介绍及分析,并在此基础上就发展中的一些问题进行讨论并提出系列建议。},
  keywords      = {网络会计;;发展问题;;对策},
}

@Article{王全睿2011,
  author        = {王全睿},
  title         = {网络会计的发展初探},
  journal       = {现代经济信息},
  year          = {2011},
  number        = {02},
  pages         = {131},
  __markedentry = {[Administrator:6]},
  abstract      = {随着国际互联网的出现,企业在全球范围内的信息交流和信息共享成为可能,会计是随着社会生产的发展和经济管理的要求而产生、发展并不断完善起来的。传统的手工会计核算与互联网技术的结合,孕育了网络会计这个新型学科的诞生。与之相适应的未来的会计模式必将以网络会计的形式出现。但是网络会计在实际运用中也会存在不足,本文就网络会计的定义、目标,以及网络会计在发展中需要解决的问题及处理对策等阐述了一些观点。},
  keywords      = {网络会计;;网络财务;;电算化会计},
}

@Article{刘湘云2011,
  author        = {刘湘云 and 邱乐平},
  title         = {Shibor已成为我国货币市场基准利率了吗?},
  journal       = {金融理论与实践},
  year          = {2011},
  number        = {01},
  pages         = {24--27},
  __markedentry = {[Administrator:6]},
  abstract      = {为培育我国货币市场基准利率体系,中国人民银行于2007年1月4日推出了上海银行间同业拆借利率(Shibor),至今,Shibor已正式运行了三年半。我们从Shibor是否具备基准利率的四个属性的角度出发,采用Granger因果关系检验和脉冲响应分析等方法,对其是否已具备承担我国货币市场基准利率的重任作了分析和检验。研究结果表明,三个月内的Shibor,其市场性、基础性和稳定性均基本具备,可以承担作为金融产品定价基准利率的功能,然而相关性的缺乏使其现在还不能够承担货币政策价格调控的基准利率这一职能。},
  keywords      = {货币市场基准利率;;Shibor;;市场性;;基础性;;稳定性;;相关性},
}

@Article{杨宝臣2010,
  author        = {杨宝臣 and 苏云鹏},
  title         = {SHIBOR市场利率期限结构实证研究},
  journal       = {电子科技大学学报(社科版)},
  year          = {2010},
  volume        = {12},
  number        = {05},
  pages         = {39--45},
  __markedentry = {[Administrator:6]},
  abstract      = {基于2006年10月8日～2008年10月9日间上海银行间同业拆放利率市场的日数据,利用单位根和协整检验对预期理论对于利率期限结构不同部分的适用性差异进行了研究,发现上海银行间同业拆放利率系统存在两个随机趋势,即短端利率的波动趋势和长端利率的线性漂移趋势,因此预期理论对于上海银行间同业拆放利率市场的利率期限结构整体上是不适用的,但是对于期限结构的短端和长端则分别适用。在此基础上,分别对短端利率和长端利率两个子系统建立了误差修正模型以刻画其动态特性,发现货币政策的效应沿上海银行间同业拆放利率期限结构衰减,从而会造成收益率曲线产生非平行变动,甚至扭曲。此外,还发现隔夜拆借利率的动态特性表现出背离长期均衡关系的趋势。},
  keywords      = {预期理论;;上海银行间同业拆放利率;;利率期限结构;;协整检验;;误差修正模型},
}

@PhdThesis{臧福江2010,
  author        = {臧福江},
  title         = {基于JJ协整检验与ECM对中国出口的实证分析},
  school        = {中国科学技术大学},
  year          = {2010},
  __markedentry = {[Administrator:6]},
  abstract      = {


出口对于我国宏观经济的发展具有非常重要的作用。本文基于Johansen协整检验以及误差修正模型,主要研究了我国的实际出口与外国的实际收入、中国对国外出口的相对出口价格、人民币汇率的波动性三者之间的长短期关系。



本文的创新之处主要有以下三点:第一,使用了月度高频数据,而不像大多数已有研究文献那样使用的是季度数据,这就使本文所得出的结论稳健性有了较大的提高;第二,本文对数据进行了更新,特别是包含了美国次贷危机全面爆发以来的最新数据,这使我们能够准确的研究美国次贷危机乃至全球金融危机对我国出口贸易的影响;第三,我们在建模时考虑了2002年12月我国正式加入世界贸易组织和2005年7月21日起我国实施的人民币汇率制度改革两个结构性的影响,这样建立的出口模型更加符合我国的实际,基于这样的模型得出的结论也更加准确。



本文主要得出四点结论:第一,Johansen协整检验显示我国的实际出口与外国的实际收入、中国对国外出口的相对出口价格之间存在一个长期稳定的均衡关系。第二,中国出口的收入价格弹性很高高。这说明我国的经济发展过度依赖外需推动,经济增长的内生动力不足。第三,中国出口的价格弹性也非常高。这就充分说明了我国出口企业在国际市场上主要依靠价格优势进行竞争。第四,ECM模型的估计结果显示较大的人民币汇率波动性能够导致我国实际外贸出口额的减少。第五,与采用季度数据相比,使用月度高频数据得出的结论从总体上看更能突出反映我国经济及出口产业面临的深层次问题。



最后,本文给出了七条有针对性的政策建议。},
  keywords      = {出口;;外国实际收入;;中国相对出口价格;;人民币汇率波动性;;金融危机;;Johansen协整检验;;误差修正模型},
}

@PhdThesis{肖明月2010,
  author        = {肖明月},
  title         = {浙江进口贸易对经济增长的影响研究},
  school        = {浙江理工大学},
  year          = {2010},
  __markedentry = {[Administrator:6]},
  abstract      = {


第二次世界大战以来,对外贸易对经济增长的影响成了经济学界研究和议论的热点问题,而研究的重点主要是放在出口对经济增长的影响上。但是对外贸易是包括出口和进口两个方面的,只对对外贸易的一个方面加以研究显然是不够的,肯定不能全面地反映对外贸易对经济增长的影响和作用。基于上面这种情况,本文主要研究进口贸易对经济增长的影响。



本文选取浙江省的进口贸易作为研究范围,试图以新增长理论为主要的理论基础,从理论分析与实证检验两个层面对浙江省进口贸易与经济增长的关系进行研究。本文开头是相关理论和研究结果的回顾,其次就是本文的重点即实证分析。在实证分析方面,首先运用协整分析方法分析两者之间的关系,然后根据国民收入恒等式建立一个包括消费、投资、进出口方程的联立方程组模型系统地对浙江省进口贸易与经济增长之间的关系进行分析,最后再分析进口贸易和全要素生产率(TFP)之间的关系。实证结果表明浙江省的进口贸易和经济增长之间存在短期和长期的相关关系,进口贸易对于浙江的经济增长不仅有直接的贡献,而且还会间接地作用于长期的经济增长。进口贸易会通过促进技术进步来促进浙江省的经济增长。},
  keywords      = {浙江;;进口贸易;;经济增长;;协整},
}

@Article{周玲2010,
